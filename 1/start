 ##   ##  ##     ## ##     ## ##   ###
##     ## ##     ## ##     ## ##    ##



z=$1
count1=$(wc -l ${1} | cut -d ' ' -f1 )
echo $count1
i=10
fif=100
p=5060
while [ $i -le $count1 ]
do
k=$(($i + $fif))

while [ $i -le $k ]
do
if [ $i -gt $count1 ]; then
break
fi
sed -n ''${i}'p' $1 >> tmpfile.txt
i=`expr $i + 1`
done




filecontent=( `cat tmpfile.txt `)

for s in "${filecontent[@]}"
do

python svmap.py -t0.0 $s -v -p $p >> $2  &
#p=`expr $p + 1`

done

	echo "======================================== ";
echo " ######   ######     ###    ##    ## ##    ## ######## ########  ";
echo "##    ## ##    ##   ## ##   ###   ## ###   ## ##       ##     ## ";
echo "##       ##        ##   ##  ####  ## ####  ## ##       ##     ## ";
echo " ######  ##       ##     ## ## ## ## ## ## ## ######   ########  ";
echo "      ## ##       ######### ##  #### ##  #### ##       ##   ##   ";
echo "##    ## ##    ## ##     ## ##   ### ##   ### ##       ##    ##  ";
echo " ######   ######  ##     ## ##    ## ##    ## ######## ##     ## ";
	echo "======================================== ";
    echo "Total Scan is ".$i;
	echo "======================================== 	                                                                ";
echo -e '\E[1;31m'"\033[1m ##     ##    ###    ########  ########       ########  ##    ##       ##     ## ##     ##    ###    ##    ## \033[0m"
echo -e '\E[1;31m'"\033[1m ###   ###   ## ##   ##     ## ##             ##     ##  ##  ##         ##   ##  ###   ###   ## ##   ###   ## \033[0m"
echo -e '\E[1;32m'"\033[1m #### ####  ##   ##  ##     ## ##             ##     ##   ####           ## ##   #### ####  ##   ##  ####  ## \033[0m"
echo -e '\E[1;32m'"\033[1m ## ### ## ##     ## ##     ## ######         ########     ##             ###    ## ### ## ##     ## ## ## ## \033[0m"
echo -e '\E[1;33m'"\033[1m ##     ## ######### ##     ## ##             ##     ##    ##            ## ##   ##     ## ######### ##  #### \033[0m"
echo -e '\E[1;33m'"\033[1m ##     ## ##     ## ##     ## ##             ##     ##    ##           ##   ##  ##     ## ##     ## ##   ### \033[0m"
echo -e '\E[1;33m'"\033[1m ##     ## ##     ## ########  ########       ########     ##          ##     ## ##     ## ##     ## ##    ## \033[0m"

	echo " ======================================== ";
	clear
rm tmpfile.txt
sleep 10
done





















unt1";
echo "Map Checker Is  Done ...$i  OFF ...$count1";
echo "Map Checker Is  Done ...$i  OFF ...$count1";
echo "Map Checker Is  Done ...$i  OFF ...$count1";
echo "#################################################################";
rm tmpfile.txt
sleep 20
done








